{
  "dashboard": {
    "title": "AI Backend - Cache Efficiency",
    "tags": ["ai-backend", "cache", "redis"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Cache Hit Rate (Overall)",
        "type": "gauge",
        "targets": [
          {
            "expr": "rate(redis_keyspace_hits_total[5m]) / (rate(redis_keyspace_hits_total[5m]) + rate(redis_keyspace_misses_total[5m])) * 100",
            "legendFormat": "Hit Rate"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 50, "color": "yellow"},
                {"value": 70, "color": "green"},
                {"value": 90, "color": "blue"}
              ]
            }
          }
        },
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        }
      },
      {
        "id": 2,
        "title": "Cache Operations",
        "type": "stat",
        "targets": [
          {
            "expr": "redis_commands_processed_total",
            "legendFormat": "Total Commands"
          }
        ],
        "gridPos": {"h": 4, "w": 8, "x": 8, "y": 0}
      },
      {
        "id": 3,
        "title": "Cache Memory Usage",
        "type": "gauge",
        "targets": [
          {
            "expr": "redis_memory_used_bytes / redis_memory_max_bytes * 100",
            "legendFormat": "Memory Usage"
          }
        ],
        "gridPos": {"h": 4, "w": 8, "x": 16, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 70, "color": "yellow"},
                {"value": 90, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Cache Keys Count",
        "type": "stat",
        "targets": [
          {
            "expr": "redis_db_keys",
            "legendFormat": "Total Keys"
          }
        ],
        "gridPos": {"h": 4, "w": 8, "x": 8, "y": 4}
      },
      {
        "id": 5,
        "title": "Evicted Keys",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(redis_evicted_keys_total[5m])",
            "legendFormat": "Evictions/sec"
          }
        ],
        "gridPos": {"h": 4, "w": 8, "x": 16, "y": 4},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 10, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Cache Hit vs Miss Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(redis_keyspace_hits_total[1m])",
            "legendFormat": "Hits/sec"
          },
          {
            "expr": "rate(redis_keyspace_misses_total[1m])",
            "legendFormat": "Misses/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "fieldConfig": {
          "defaults": {
            "custom": {"drawStyle": "line", "lineWidth": 2}
          }
        }
      },
      {
        "id": 7,
        "title": "Cache Latency",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(redis_command_duration_seconds_bucket[5m]))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(redis_command_duration_seconds_bucket[5m]))",
            "legendFormat": "P99"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      },
      {
        "id": 8,
        "title": "Cache Size Growth",
        "type": "timeseries",
        "targets": [
          {
            "expr": "redis_memory_used_bytes",
            "legendFormat": "Memory Used"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        }
      },
      {
        "id": 9,
        "title": "Keys by Database",
        "type": "piechart",
        "targets": [
          {
            "expr": "redis_db_keys",
            "legendFormat": "db{{db}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 10,
        "title": "LiteLLM Cache Savings",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(litellm_cache_hits_total) / (sum(litellm_cache_hits_total) + sum(litellm_cache_misses_total)) * 100",
            "legendFormat": "Cache Efficiency"
          }
        ],
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 24},
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "custom": {"neutral": 0},
            "mappings": [],
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 50, "color": "yellow"},
                {"value": 80, "color": "green"}
              ]
            }
          }
        },
        "options": {
          "textMode": "value_and_name",
          "colorMode": "background"
        }
      }
    ]
  }
}
