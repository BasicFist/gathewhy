{
  "title": "Performance & Latency Analysis",
  "tags": [
    "litellm",
    "performance",
    "latency"
  ],
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Latency Comparison by Provider",
      "type": "graph",
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "rate(litellm_request_total_latency_metric[5m])",
          "legendFormat": "{{api_provider}} - total",
          "refId": "A"
        },
        {
          "expr": "rate(litellm_llm_api_latency_metric[5m])",
          "legendFormat": "{{api_provider}} - API only",
          "refId": "B"
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "Latency"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 2,
      "title": "P95 Latency by Model",
      "type": "graph",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 10
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(litellm_request_total_latency_metric_bucket[5m]))",
          "legendFormat": "{{model}}",
          "refId": "A"
        }
      ],
      "yaxes": [
        {
          "format": "s",
          "label": "P95 Latency"
        },
        {
          "format": "short"
        }
      ]
    },
    {
      "id": 3,
      "title": "Latency Heatmap",
      "type": "heatmap",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 10
      },
      "targets": [
        {
          "expr": "rate(litellm_request_total_latency_metric_bucket[5m])",
          "refId": "A"
        }
      ]
    },
    {
      "id": 4,
      "title": "Provider Performance Comparison",
      "type": "table",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 18
      },
      "targets": [
        {
          "expr": "avg by (api_provider) (rate(litellm_request_total_latency_metric[5m]))",
          "refId": "A",
          "format": "table"
        },
        {
          "expr": "sum by (api_provider) (rate(litellm_proxy_total_requests_metric[5m]))",
          "refId": "B",
          "format": "table"
        }
      ]
    }
  ]
}
